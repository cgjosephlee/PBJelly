<jellyProtocol>
    <reference>/FULL/PATH/TO__/PBJelly/data/reference/lambda.fasta</reference>  
    <outputDir>/FULL/PATH/TO__/PBJelly/lambdaExample/</outputDir>
    <cluster>
        <command>echo '${CMD}' | msub -N "${JOBNAME}" -o ${STDOUT} -e ${STDERR} -l nodes=1:ppn=8,mem=48000mb</command>
        <nJobs>1</nJobs>
    </cluster>
    <blasr>-minMatch 8 -minPctIdentity 70 -bestn 8 -nCandidates 30 -maxScore -500 -nproc 8 -noSplitSubreads</blasr>
    <input baseDir="/FULL/PATH/TO__/PBJelly/lambdaExample/data/reads/">
        <job>pacbioReads.fasta</job>
    </input>
</jellyProtocol>
